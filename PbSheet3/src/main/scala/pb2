package solutions

/*
init: S = []
*/
trait Stack[A] {

  /** Adds element to the stack
    * post: S = x ++ S */
  def push(x : A)
  

  /** Removes element from the stack
    * pre: isEmpty()!=0
    * post: S = tail S ^ return head SS*/
  def pop() : A

  /** Checks if empty
    * post: S = S ^ return S == [] */
  def isEmpty()
}
